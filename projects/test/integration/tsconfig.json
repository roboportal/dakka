{
  "include": [
    "./",
    "./mocks/**/*",
    "../../../node_modules/@roboportal/export_processors/**/*",
    "../../../node_modules/@roboportal/constants/**/*",
    "../../../node_modules/@roboportal/utils/**/*"
  ],
  "compilerOptions": { 
    "target": "es2021",
    "outDir": "dist",
    "moduleResolution": "node",
    "module": "commonjs",
    "baseUrl": "./",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}